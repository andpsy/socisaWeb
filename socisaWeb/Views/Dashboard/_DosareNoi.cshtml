@model SOCISA.Models.DosarExtended[]

<div style="width:100%;overflow:auto;" ng-init="@String.Format("model = {0}", Newtonsoft.Json.JsonConvert.SerializeObject(Model))">
    <select class = "form-control input-sm" ng-model="query" ng-change="applyFilter()">
        <option value="1">Toate dosarele noi</option>
        <option value="2">Dosare noi CASCO</option>
        <option value="3">Dosare noi RCA</option>
    </select>
    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <!--
                <th><input type="checkbox" id="chk_All" checked onchange="toggleChecks(this)" /></th>
                -->
                <th>NUMAR DOSAR CASCO</th>
                <th>NUMAR SCA</th>
                <th>DATA SCA</th>
                <th>NUMAR POLITA CASCO</th>
                <th>DATA EVENIMENT</th>
                <th>SOCIETATE CASCO</th>
                <th>ASIGURAT CASCO</th>
                <th>NUMAR AUTO CASCO</th>
                <th>NUMAR POLITA RCA</th>
                <th>SOCIETATE RCA</th>
                <th>ASIGURAT RCA</th>
                <th>NUMAR AUTO RCA</th>
                <th>VALOARE DAUNA</th>
                <th>VALOARE REGRES</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="item in model | filter:applyFilter">
                <!--
                <td><input type="checkbox" checked class="checkForImport" id="chk_{{$index}}" ng-model="item.selected" /></td>
                -->
                    <td><a
                <td><a href="#" title="Detalii dosar">{{item.Dosar.NR_DOSAR_CASCO}}</a></td>
                <td>{{item.Dosar.NR_SCA}}</td>
                <td>{{item.Dosar.DATA_SCA | date:"dd.MM.yyyy"}}</td>
                <td>{{item.Dosar.NR_POLITA_CASCO}}</td>
                <td>{{item.Dosar.DATA_EVENIMENT | date:"dd.MM.yyyy"}}</td>
                <td>{{item.SocietateCasco.DENUMIRE_SCURTA}}</td>
                <td>{{item.AsiguratCasco.DENUMIRE}}</td>
                <td>{{item.AutoCasco.NR_AUTO}}</td>
                <td>{{item.Dosar.NR_POLITA_RCA}}</td>
                <td>{{item.SocietateRca.DENUMIRE_SCURTA}}</td>
                <td>{{item.AsiguratRca.DENUMIRE}}</td>
                <td>{{item.AutoRca.NR_AUTO}}</td>
                <td>{{item.Dosar.VALOARE_DAUNA}}</td>
                <td>{{item.Dosar.VALOARE_REGRES}}</td>
            </tr>
        </tbody>
    </table>
    <div class="docsRightMenu" style="right:286px;">
        <div class="docsRightMenuButton" onclick="ToggleDiv('mainDosareNoi')">&#9668;</div>
    </div>
</div>