                            <a href="#" class="btn btn-default" title="Navigare la primul dosar" ng-click="firstDosar()">|&lt;</a>
                            <a href="#" class="btn btn-default" title="Navigare la dosarul anterior" ng-click="prevDosar()">&lt;</a>
