@using socisaWeb.Helpers
@model socisaWeb.DosarView
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <!-- Lista dosare -->
    <div id="dosareSideNav" style="width:0px;height:0px;overflow-y:auto;position:absolute;top:0px;left:0px;margin:5px;border-right:1px solid black;background-color:#111;opacity:0.9;filter:alpha(opacity=90);z-index:3;transition: 0.5s;">
        <table style="width:95%;padding:5px;margin:5px;">
            <tr style="border-bottom:1px solid white;">
                <td style="padding:5px;">Lista dosare:</td>
                <td style="padding:5px;text-align:right"><a href="#" onclick="showDosareSideNav(false)">X</a></td>
            </tr>
            <tr ng-show="DosarFiltru.DosareResult == null || compareObjects(DosarFiltru.DosareResult, {})">
                <td colspan="2" style="padding:5px;">
                    Nu sunt dosare filtrate
                </td>
            </tr>
            <tr ng-repeat="dosar in DosarFiltru.DosareResult">
                <td ng-show="$index == curDosarIndex" colspan="2" style="padding:5px;color:red;font-weight:bold;">
                    Numar Dosar CASCO: <a href="#" ng-click="ShowDosar($index);">{{dosar.NR_DOSAR_CASCO}}</a><br />
                    Numar SCA: <b>{{dosar.NR_SCA}} / {{dosar.DATA_SCA | dateFilter | date : DATE_FORMAT}}</b>
                </td>
                <td ng-show="$index != curDosarIndex" colspan="2" style="padding:5px;">
                    Numar Dosar CASCO: <a href="#" ng-click="ShowDosar($index);">{{dosar.NR_DOSAR_CASCO}}</a><br />
                    Numar SCA: <b>{{dosar.NR_SCA}} / {{dosar.DATA_SCA | dateFilter | date : DATE_FORMAT}}</b>
                </td>
            </tr>
        </table>
    </div>
