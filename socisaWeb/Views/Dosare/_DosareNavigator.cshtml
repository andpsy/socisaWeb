                            <a href="#" class="btn btn-default" title="Afisare lista dosare gasite" onclick="showDosareSideNav(true)">&equiv;</a>
                            <a href="#" class="btn btn-default" title="Navigare la dosarul urmator" ng-click="nextDosar()">&gt;</a>
